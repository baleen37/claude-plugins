{
  "name": "lsp-go",
  "version": "5.6.1",
  "description": "Language Server Protocol support for Go - provides gopls integration for Go code completion, diagnostics, and navigation",
  "author": {
    "name": "baleen37",
    "email": "git@baleen.me"
  },
  "keywords": [
    "lsp",
    "go",
    "golang",
    "gopls",
    "autocomplete",
    "diagnostics"
  ],
  "lspServers": {
    "go": {
      "command": "gopls",
      "args": [
        "serve"
      ],
      "extensionToLanguage": {
        ".go": "go"
      }
    }
  }
}
