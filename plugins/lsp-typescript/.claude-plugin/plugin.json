{
  "name": "lsp-typescript",
  "version": "5.3.3",
  "description": "TypeScript/JavaScript Language Server integration for Claude Code",
  "author": {
    "name": "baleen37"
  },
  "license": "MIT",
  "keywords": [
    "lsp",
    "typescript",
    "javascript",
    "autocomplete",
    "diagnostics",
    "tsx",
    "jsx"
  ],
  "lspServers": {
    "typescript": {
      "command": "typescript-language-server",
      "args": [
        "--stdio"
      ],
      "extensionToLanguage": {
        ".ts": "typescript",
        ".tsx": "typescript",
        ".js": "javascript",
        ".jsx": "javascript",
        ".mjs": "javascript",
        ".cjs": "javascript"
      }
    }
  }
}
