{
  "name": "claude-plugins",
  "version": "5.5.0",
  "description": "Baleen Claude Plugins - Claude Code plugin collection - Testing semantic-release workflow",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky",
    "commit": "git-cz",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:bats": "bash tests/run-all-tests.sh",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@semantic-release/git": "^10.0.1",
    "@vitest/ui": "^2.0.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "gray-matter": "^4.0.0",
    "husky": "^9.1.7",
    "semantic-release": "^21.0.7",
    "vitest": "^2.0.0",
    "yaml": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1"
  }
}
